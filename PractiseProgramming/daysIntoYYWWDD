#include <stdio.h>

int main() {
    int days;
    int week = 0;
    int year = 0;
    printf("Enter days :");
    scanf("%d",&days);
    
    year = days / 365;
    days = days % 365;
    
    week = days / 7;
    days = days % 7;
    
    printf("Year: %d week: %d days: %d", year, week, days);
    return 0;
}